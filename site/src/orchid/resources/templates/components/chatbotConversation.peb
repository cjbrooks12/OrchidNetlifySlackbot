<h2 class="special" id="interactive-example">Interactive Example</h2>
<div>
  <div class="channel-body">
    <div class="wrapper" style="display: none">
      <div class="main-container">
        <div class="sidebar">
          <ul class="channel-list">
          {{ slackChannel() }}
          {{ slackChannel() }}
            <li><br></li>
          {{ slackChannel() }}
          {{ slackChannel() }}
          {{ slackChannel() }}
          {{ slackChannel() }}
          </ul>
        </div>
        <div class="channel-messages">

        </div>
        <div class="channel-footer">
        {{ slackFooter() }}
        </div>
      </div>
    </div>
  </div>
</div>

{% macro slackChannel() %}
<li class="channel-item">
  <span class="bullet">&nbsp;</span>
  <span class="text">&nbsp;</span>
</li>
{% endmacro %}

{% macro slackFooter() %}
<form class="input-wrapper">
  <button class="input-button" type="submit">
    <i class="fa fa-plus" aria-hidden="true"></i>
  </button>
  <input class="input-field" placeholder="Message #crederabot" type="text">
</form>
{% endmacro %}

